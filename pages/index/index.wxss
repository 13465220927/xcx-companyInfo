page{
    width:100%;
    background:#f5f5f5;
}

.specify{
    position:fixed;
    width:100rpx;
    height:100rpx;
    font-size:12px;
    text-align:center;
    right:10rpx;
    top:40%;
}

.startImg{
    position:fixed;
    left:0;
    top:0;
    width:100%;
    height:100vh;
    animation:fadeNone 1s ease;
    animation-fill-mode: forwards;
    animation-delay: 3s;
    z-index:99;
}
.startImg image:nth-child(1){
     width:150rpx;
     height:150rpx;
     position: absolute;
     top:100rpx;
     left:50%;
     transform:translateX(-50%);
}
.startImg image:nth-child(2){
    width:100%;
    height:100%;
}
@keyframes fadeNone{
    to{
       opacity:0;
    }
}