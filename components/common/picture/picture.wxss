.pics{
    display:flex;
    align-items: center;
    flex-wrap:wrap;
    width:100%;
    position: relative;
}
.pics .picItem{
    width:33.3%;
    height:100rpx;
    padding:0rpx 10rpx 10rpx 0;
}
.picItem image{
    width:100%;
    height:100rpx;
}
.pics .morePic{
    position: absolute;
    right:10rpx;
    top:0;
    width:30%;
    height:100rpx;
    background:rgba(0,0,0,.4);
    color:white;
    line-height:100rpx;
    text-align:center;
}