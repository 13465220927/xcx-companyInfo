.pics{
    display:flex;
    align-items: center;
    flex-wrap:wrap;
    width:420rpx;
    position: relative;
}
.pics .picItem{
    width:140rpx;
    height:140rpx;
    padding:0 10rpx 10rpx 0;
}
.picItem image{
    width:100%;
    height:100%;
    display:block;
    margin:0 auto;
   
}
.pics .morePic{
    position: absolute;
    right:10rpx;
    top:0;
    width:140rpx;
    height:140rpx;
    background:rgba(0,0,0,.4);
    color:white;
    line-height:140rpx;
    text-align:center;
}