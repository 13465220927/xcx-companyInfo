.header-wrap{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-wrap:wrap;
    background:white;
    border-top:1px solid #efefef;
    min-height:180rpx;
    padding-top:28rpx;
}
.header-wrap{
    text-align: center;
    font-size:12px;
    color:#262626;
}
.header-wrap .item{
    flex:1;
    min-width: 20%;
    margin-bottom:28rpx;
}
.header-wrap image{
    width:82rpx;
    height:82rpx;
    display:block;
    margin:0 auto 10rpx;

} 
