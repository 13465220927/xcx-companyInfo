.notice-wrap{
    height:176rpx;
    background:white;
    display: flex;
    align-items: center;
    padding:20rpx 58rpx 40rpx 20rpx;
    margin-top:16rpx;
    margin-bottom:10rpx;
    position: relative;
}

.notice-wrap .noticeSign{
   width:72rpx;
   height:79rpx;
   margin-right:20rpx;
 
}

.notice-wrap .notice{
    flex:1;
    padding-top:20rpx;
}
.notice .item{
    font-size:12px;
    color:#363636;
    height:30rpx;
    line-height:32rpx;
    margin-bottom:14rpx;
    position: relative;
    padding-left:20rpx;
}
.notice .item .time{
  position: absolute;
  right:0;
}
.notice .seeMore{
    position: absolute;
    bottom:0;
    right:45rpx;
    color:#ff3636;
    font-size:10px;
}