<view class="banner-wrap">
  <image class="myqy" src="../../../img/index/myqy.jpg" /> 
  <swiper class="banner" indicator-dots="{{false}}"
    autoplay="true" interval="2000" duration="2000">
    <block wx:for="{{bannerImg}}" wx:key="{{index}}">
      <swiper-item>
        <image src="{{item.img}}" class="slide-image" width="355" height="150"/>
      </swiper-item>
    </block>
  </swiper>
</view>
